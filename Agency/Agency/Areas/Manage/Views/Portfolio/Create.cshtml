@using Agency.Models
@model Portfolio
<div class="container">
    <form method="post" encypte="multipart/form-data">
        <div class="mb-3">
            <label asp-for="FullName" class="form-label">Fullname</label>
            <input asp-for="FullName" class="form-control">
           <span asp-validation-for="FullName" style="color:aqua"></span>
            <div class="mb-3">
                <label asp-for="Position" class="form-label">Position</label>
                <input asp-for="Position" class="form-control">
                <span asp-validation-for="Position" style="color:aqua"></span>
                <div class="mb-3">
                    <label asp-for="PhotoFile" class="form-label">PhotoFile</label>
                    <input asp-for="PhotoFile" class="form-control">
                    <span asp-validation-for="PhotoFile" style="color:aqua"></span>
      
       
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>
